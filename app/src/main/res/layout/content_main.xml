<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="dam.isi.frsf.utn.edu.ar.laboratorio09.MainActivity"
    tools:showIn="@layout/activity_main">

    <TextView
        android:text="Hora Máximo X:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView"/>

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView"
        android:layout_toEndOf="@+id/textView"
        android:id="@+id/textViewHoraX"/>

    <TextView
        android:text="Magnitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:id="@+id/textView3"/>

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignEnd="@+id/textViewHoraX"
        android:id="@+id/textViewMagnitudX"/>

    <TextView
        android:text="Hora Máximo Y:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:id="@+id/textView13"
        android:paddingTop="10dp"/>

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewMagnitudX"
        android:layout_alignEnd="@+id/textViewMagnitudX"
        android:id="@+id/textViewHoraY"
        android:paddingTop="10dp"/>

    <TextView
        android:text="Magnitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true"
        android:id="@+id/textView15"/>

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewHoraY"
        android:layout_alignStart="@+id/textViewHoraY"
        android:id="@+id/textViewMagnitudY"/>

    <TextView
        android:text="Hora Máximo Z:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView15"
        android:layout_alignParentStart="true"
        android:id="@+id/textView17"
        android:paddingTop="10dp"/>

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewMagnitudY"
        android:layout_alignEnd="@+id/textViewMagnitudY"
        android:id="@+id/textViewHoraZ"
        android:paddingTop="10dp"/>

    <TextView
        android:text="Magnitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_alignParentStart="true"
        android:id="@+id/textView19"/>

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewHoraZ"
        android:layout_alignStart="@+id/textViewHoraZ"
        android:id="@+id/textViewMagnitudZ"/>
</RelativeLayout>
